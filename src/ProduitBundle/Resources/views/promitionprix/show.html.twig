{% extends 'layoutback.html.twig' %}

{% block body %}
    <h1>Promitionprix</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ promitionPrix.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ promitionPrix.nom }}</td>
            </tr>
            <tr>
                <th>Datedebut</th>
                <td>{% if promitionPrix.dateDebut %}{{ promitionPrix.dateDebut|date('Y-m-d') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Datefin</th>
                <td>{% if promitionPrix.dateFin %}{{ promitionPrix.dateFin|date('Y-m-d') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Pourcentage</th>
                <td>{{ promitionPrix.pourcentage }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('promitionprix_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('promitionprix_edit', { 'id': promitionPrix.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
